{
    "name": "json-helper",
    "displayName": "JSON Helper",
    "description": "Give you appropriate descriptions for JSON keys",
    "license": "MIT",
    "version": "1.1.3",
    "publisher": "zhoufeng",
    "repository": {
        "type": "git",
        "url": "https://github.com/mine2chow/JSON-Helper.git"
    },
    "engines": {
        "vscode": "^1.20.0"
    },
    "categories": [
        "Languages"
    ],
    "activationEvents": [
        "onLanguage:json"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands":[
            {
                "command": "jsonHelper.docLink",
                "title": "Link to documentation"
            },
            {
                "command": "jsonHelper.copyToClipboard",
                "title": "Copy text to clipboard"
            }
        ],
        "menus": {
            "commandPalette": [
                {
                    "command": "jsonHelper.docLink",
                    "when": "false"
                },
                {
                    "command": "jsonHelper.copyToClipboard",
                    "when": "false"
                }
            ]
        },
        "configuration": {
            "title": "JSON helper configuration",
            "properties": {
                "jsonHelper.object.name": {
                    "type": "string",
                    "default": "jsonObj",
                    "description": "JSON object name for showing",
                    "scope": "window"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "",
        "compile": "tsc -p ./",
        "watch": "tsc -w -p ./",
        "update-vscode": "node ./node_modules/vscode/bin/install",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "dependencies": {
        "vscode": "^1.1.14",
        "jsonc-parser": "^0.4.2",
        "clipboardy": "^1.2.3"
    },
    "devDependencies": {
        "@types/node": "9.4.6",
        "typescript": "2.7.2"
    },
    "icon": "imgs/JSON-icon.gif",
    "homepage": "https://github.com/mine2chow/JSON-Helper/blob/master/README.md"
}
